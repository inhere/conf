#
# My git config. at  ~/.gitconfig
#
# @link https://github.com/inhere/conf
#

[alias]
        co = checkout
        ci = commit
        br = branch
        st = status
        pul = pull
[push]
        default = simple
[core]
        filemode = false
        autocrlf = input
        ignorecase = false
[credential]
        helper = cache
